<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.MemberServiceDao">
    <insert id="insertMService" parameterType="MemberService">
      INSERT INTO service_profile
        (id,name,gender,age)
       VALUES
        (#{service.id},#{service.name},#{service.gender},#{service.age})
    </insert>

</mapper>